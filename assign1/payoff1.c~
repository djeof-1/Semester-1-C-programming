     printf("cost of modification is\n");
     scanf("%f",&c);
     printf("mpg before modification is\n");
     scanf("%f",&d);
     printf("mpg after modification is\n");
     scanf("%f",&e);
	 f=(a/d)*(b);
	 printf("cost of driving without modification in $ is %3.2f\n",f);
     g=(a/e)*(b);
     printf("cost of driving with modification in $ is %3.2f\n",g);
     h=c/(f-g);
     x=h;
     z=(h-x);
     y=z*12;
     p=z-y;
     k=(z-y)*30;
     l=p-k;
     m=l*24;
      printf("%d,%d,%d,%d",x,y,k,m);
